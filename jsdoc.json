{
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)[_']"
  },
  "opts": {
    "destination": "./doc",
    "private": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true
  },
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  }
}
